module SwiftMessage.Util where

data MessageStatus = Pending | Finalized | Processed | Failed String
  deriving (Eq, Show)

data MessageType = 
    CANC -- Cancellation Request
  | NEWM -- New
  | RVSL -- Reversal
  | REPE
  | REPL
  | RMDR
  deriving (Eq, Show)

data CRDB = CR | DB deriving (Eq, Show)